{
  "name": "rc-bezier",
  "version": "0.1.2",
  "license": "MIT",
  "author": "boenfu",
  "source": "src/library/index.tsx",
  "main": "bld/library/index.js",
  "module": "bld/library/index.js",
  "types": "bld/library/index.d.ts",
  "scripts": {
    "prepublishOnly": "yarn test",
    "prebuild": "rimraf ./bld",
    "build": "microbundle build --css-modules false --jsx React.createElement -f es",
    "doc": "rimraf ./dist && parcel build examples/index.html --no-source-maps --no-content-hash --public-url /bezier/ && typedoc --theme minimal --out dist/docs src",
    "lint": "eslint .",
    "lint-prettier": "prettier --check .",
    "start": "parcel examples/index.html --open",
    "gh-pages": "yarn doc && gh-pages -d dist",
    "test": "yarn lint-prettier && yarn build && yarn lint"
  },
  "files": [
    "src/library/**",
    "bld/library/**"
  ],
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@mufan/code": "^0.2.5",
    "@mufan/eslint-plugin": "^0.1.37",
    "@types/lodash-es": "^4.17.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-tabs": "^2.3.2",
    "eslint": "^7.15.0",
    "gh-pages": "^3.1.0",
    "microbundle": "^0.13.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-tabs": "^3.1.2",
    "rimraf": "^3.0.2",
    "typedoc": "^0.20.0-beta.27",
    "typescript": "^4.1.3"
  }
}
